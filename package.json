  "version": "0.0.2",
  "description": "Simple collapsible card custom element with header and actions",
  "main": "index.js",
  "type": "module",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "prepublishOnly": "tsc",
    "start": "wds --open --watch",
